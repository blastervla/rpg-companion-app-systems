        {
          "system": "5e2024",
          "resource_id": "spell",
          "stats": {
            "name": {
              "value": "Cloudkill"
            },
            "source": {
              "value": "phb"
            },
            "level": {
              "value": "spell_level_5"
            },
            "school": {
              "value": "spell_school_conjuration"
            },
            "casting_time": {
              "value": "1 action"
