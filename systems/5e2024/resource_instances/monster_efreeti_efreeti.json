        },
        {
          "system": "5e2024",
          "resource_id": "spell",
          "stats": {
            "name": {
              "value": "Gaseous Form"
            },
            "source": {
              "value": "phb"
            },
            "level": {
              "value": "spell_level_3"
            },
            "school": {
              "value": "spell_school_transmutation"
