          "stats": {
            "name": {
              "value": "Dancing Lights"
            },
            "source": {
              "value": "phb"
            },
            "school": {
              "value": "spell_school_illusion"
            },
            "casting_time": {
              "value": "1 action"
            },
            "range": {
