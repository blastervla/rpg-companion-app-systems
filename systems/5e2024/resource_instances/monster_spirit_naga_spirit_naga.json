        {
          "system": "5e2024",
          "resource_id": "spell",
          "stats": {
            "name": {
              "value": "Minor Illusion"
            },
            "source": {
              "value": "phb"
            },
            "school": {
              "value": "spell_school_illusion"
            },
            "casting_time": {
