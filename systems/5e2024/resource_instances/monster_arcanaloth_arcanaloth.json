        {
          "system": "5e2024",
          "resource_id": "spell",
          "stats": {
            "name": {
              "value": "Mage Hand"
            },
            "source": {
              "value": "phb"
            },
            "school": {
              "value": "spell_school_conjuration"
            },
            "casting_time": {
              "value": "1 action"
