          "stats": {
            "name": {
              "value": "Protection from Evil and Good"
            },
            "source": {
              "value": "phb"
            },
            "level": {
              "value": "spell_level_1"
            },
            "casting_time": {
              "value": "1 action"
            },
            "range": {
