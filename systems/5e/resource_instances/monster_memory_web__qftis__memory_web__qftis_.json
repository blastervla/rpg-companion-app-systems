        {
          "resource_id": "monster_trait",
          "stats": {
            "trait_name": {
              "value": "False Appearance"
            },
            "trait_description": {
              "value": "If the memory web is motionless at the start of combat, it has advantage on its initiative roll. Moreover, if a creature hasn't observed the memory web move or act, that creature must succeed on a DC 18 Intelligence (Investigation) check to discern that the memory web is animate."
            },
            "id": "9f67682b-6749-4346-a80e-a37b79b82768",
