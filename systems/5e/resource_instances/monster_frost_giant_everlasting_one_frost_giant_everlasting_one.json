          "stats": {
            "trait_name": {
              "value": "Regeneration"
            },
            "trait_description": {
              "value": "The giant regains 10 hit points at the start of its turn. If the giant takes acid or fire damage, this trait doesn't function at the start of its next turn. The giant dies only if it starts its turn with 0 hit points and doesn't regenerate."
            },
            "id": "e8f6ee8b-5d46-4a51-a891-db81bb125a93",
