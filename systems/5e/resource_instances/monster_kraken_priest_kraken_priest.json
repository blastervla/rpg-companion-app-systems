        {
          "resource_id": "spell",
          "stats": {
            "id": "create_or_destroy_water",
            "name": {
              "value": "Create or Destroy Water"
            },
            "source": {
              "value": "phb"
            },
            "level": {
              "value": "spell_level_1"
            },
            "school": {
              "value": "spell_school_transmutation"
