        {
          "resource_id": "monster_action",
          "stats": {
            "action_name": {
              "value": "Incite Fanaticism"
            },
            "action_description": {
              "value": "The abishai chooses up to four other creatures within 60 feet of it that can see it. Until the start of the abishai's next turn, each of those creatures makes attack rolls with advantage and can't be frightened."
            },
