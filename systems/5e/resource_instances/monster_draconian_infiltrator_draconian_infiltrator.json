        },
        {
          "resource_id": "monster_trait",
          "stats": {
            "trait_name": {
              "value": "Glide"
            },
            "trait_description": {
              "value": "When the draconian falls and isn't incapacitated, it subtracts up to 100 feet from the fall when calculating the fall's damage, and it can move up to 2 feet horizontally for every 1 foot it descends."
            },
