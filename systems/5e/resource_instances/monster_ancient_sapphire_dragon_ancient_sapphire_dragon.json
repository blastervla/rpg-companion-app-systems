    "spells": {
      "value": [
        {
          "resource_id": "spell",
          "stats": {
            "name": {
              "value": "Meld Into Stone"
            },
            "source": {
              "value": "phb"
            },
            "level": {
              "value": "spell_level_3"
            },
            "school": {
              "value": "spell_school_transmutation"
            },
            "ritual": {
              "value": true
            },
            "casting_time": {
