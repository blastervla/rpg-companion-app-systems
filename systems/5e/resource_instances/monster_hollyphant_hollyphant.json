        },
        {
          "resource_id": "monster_action",
          "stats": {
            "action_name": {
              "value": "Trumpet of Blasting"
            },
            "action_description": {
              "value": "Each creature in the cone must make a DC 14 Constitution saving throw. On a failed save, a creature takes 17 (5d6) thunder damage and is deafened for 1 minute. On a successful save, a creature takes half as much damage and isn't deafened. Nonmagical objects in the cone that aren't being held or worn take 35 (10d6) thunder damage."
            },
            "action_type": {
              "value": "none"
            },
            "action_attack_bonus": {
              "value": 0
            },
            "action_damage_variants": {
