        {
          "resource_id": "monster_action",
          "stats": {
            "action_name": {
              "value": "Trumpet (3/Day)"
            },
            "action_description": {
              "value": "The hollyphant blows air through its trunk, creating a trumpet sound that can be heard out to a range of 600 feet. The trumpet also creates a 30-foot cone of energy that has one of the following effects, chosen by the hollyphant:"
            },
            "action_type": {
              "value": "none"
            },
