            }
          }
        },
        {
          "resource_id": "monster_action",
          "stats": {
            "action_name": {
              "value": "Trumpet of Sparkles"
            },
            "action_description": {
              "value": "Creatures in the cone must make a DC 14 Constitution saving throw, taking 22 (4d8 + 4) radiant damage on a failed save, or half as much damage on a successful one. Evil creatures have disadvantage on the saving throw. Good creatures in the cone take no damage."
            },
            "action_type": {
              "value": "none"
            },
            "action_attack_bonus": {
              "value": 0
            },
            "action_damage_variants": {
              "value": []
            },
