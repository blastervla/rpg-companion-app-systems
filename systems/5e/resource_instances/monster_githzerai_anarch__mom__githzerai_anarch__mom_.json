        {
          "resource_id": "spell",
          "stats": {
            "id": "plane_shift",
            "name": {
              "value": "Plane Shift"
            },
            "source": {
              "value": "phb"
            },
            "level": {
              "value": "spell_level_7"
            },
            "school": {
              "value": "spell_school_conjuration"
            },
