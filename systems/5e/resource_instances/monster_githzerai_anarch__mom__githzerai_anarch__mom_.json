        {
          "resource_id": "spell",
          "stats": {
            "id": "wall_of_force",
            "name": {
              "value": "Wall of Force"
            },
            "source": {
              "value": "phb"
            },
            "level": {
              "value": "spell_level_5"
            },
            "school": {
              "value": "spell_school_evocation"
