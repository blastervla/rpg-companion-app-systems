          "stats": {
            "id": "detect_magic",
            "name": {
              "value": "Detect Magic"
            },
            "source": {
              "value": "phb"
            },
            "level": {
              "value": "spell_level_1"
            },
            "school": {
              "value": "spell_school_divination"
